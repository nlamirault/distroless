{
  extends: [
    'config:recommended',
    ':dependencyDashboard',
    ':gitSignOff',
  ],
  enabled: true,
  timezone: 'Europe/Paris',
  dependencyDashboard: true,
  dependencyDashboardTitle: 'Renovate Dashboard',
  suppressNotifications: [
    'prIgnoreNotification',
  ],
  rebaseWhen: 'conflicted',
  assignees: [
    '@nlamirault',
  ],
  reviewersFromCodeOwners: true,
  labels: [
    'kind/renovate',
    'lifecycle/active',
    'status/review_needed',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/.*py$/',
      ],
      matchStrings: [
        'datasource=(?<datasource>.*)\\sdepName=(?<depName>.*)\\n.*version:\\s(?<currentValue>.*)',
      ],
    },
  ],
  packageRules: [
    {
      matchUpdateTypes: [
        'major',
      ],
      labels: [
        'priority/high',
      ],
    },
    {
      matchUpdateTypes: [
        'minor',
      ],
      labels: [
        'priority/medium',
      ],
    },
    {
      matchUpdateTypes: [
        'patch',
      ],
      labels: [
        'priority/low',
      ],
    },
    {
      matchDatasources: [
        'docker',
      ],
      addLabels: [
        'kind/renovate',
        'dependency/docker',
        'lifecycle/active',
        'status/review_needed',
        'size/xs',
      ],
    },
    {
      matchDatasources: [
        'github-releases',
        'github-tags',
      ],
      addLabels: [
        'kind/renovate',
        'dependency/github',
        'lifecycle/active',
        'status/review_needed',
        'size/xs',
      ],
    },
    {
      matchManagers: [
        'github-actions',
      ],
      addLabels: [
        'kind/renovate',
        'dependency/github',
        'lifecycle/active',
        'status/review_needed',
        'size/xs',
      ],
    },
    {
      matchFileNames: [
        '.pre-commit-config.yaml',
      ],
      addLabels: [
        'kind/renovate',
        'dependency/github',
        'lifecycle/active',
        'status/review_needed',
        'size/xs',
      ],
    },
  ],
}
